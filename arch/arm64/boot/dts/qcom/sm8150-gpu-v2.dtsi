&soc {
	gpu_opp_table_v2: gpu_opp_table_v2 {
		compatible = "operating-points-v2";
		opp-770000000 {
			opp-hz = /bits/ 64 <770000000>;
			opp-microvolt = <296>;
		};

		opp-725000000 {
			opp-hz = /bits/ 64 <725000000>;
			opp-microvolt = <206>;
		};

		opp-692000000 {
                        opp-hz = /bits/ 64 <692000000>;
                        opp-microvolt = <206>;
                };

		opp-620000000 {
			opp-hz = /bits/ 64 <620000000>;
			opp-microvolt = <138>;
		};

		opp-535000000 {
			opp-hz = /bits/ 64 <535000000>;
			opp-microvolt = <78>;
		};

		opp-480000000 {
			opp-hz = /bits/ 64 <480000000>;
			opp-microvolt = <70>;
		};

		opp-360000000 {
			opp-hz = /bits/ 64 <360000000>;
			opp-microvolt = <24>;
		};

		opp-270000000 {
			opp-hz = /bits/ 64 <270000000>;
			opp-microvolt = <23>;
		};

		opp-180000000 {
			opp-hz = /bits/ 64 <180000000>;
			opp-microvolt = <22>;
		};
	};
};
